@{
    List<StoryModel> m = (List<StoryModel>) ViewData["tours"];
}
<!DOCTYPE html>

<html>
<head>


</head>
<body>
<div>


    <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel" style="z-index: 128" data-interval="false">

        <div class="carousel-inner">
            @{
                string gg = "active";
            }
            @foreach (var story in m)
            {
                <div class="carousel-item @gg">
                    <p class="text-white "> @story.title</p>
                    <div class="d-flex justify-content-center">
                        <img class="d-block w-25 " src="assets/background.jpg" alt="Second slide">
                    </div>
                    <div class="d-flex justify-content-center">
                        <audio controls>
                            <source src=@story.url type="audio/mpeg">
                        </audio>
                    </div>
                </div>
                gg = "";
            }

        </div>
        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>

</div>
</body>
</html>